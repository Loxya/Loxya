{
    "type": "project",
    "private": true,
    "authors": [
        {
            "name": "Loxya Team",
            "email": "technical@loxya.com"
        }
    ],
    "scripts": {
        "create-migration": "bin/console migrations:create",
        "migration-status": "bin/console migrations:status",
        "migrate": "bin/console migrations:migrate",
        "rollback": "bin/console migrations:rollback",
        "lint": "vendors/bin/phpcs",
        "lint:fix": "vendors/bin/phpcbf",
        "phpstan": "vendors/bin/phpstan analyse --memory-limit=1G",
        "test": "vendors/bin/phpunit --colors=always --testdox"
    },
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/Loxya-vendors/Validation-PHP.git"
        },
        {
            "type": "git",
            "url": "https://github.com/Loxya-vendors/PDF417-PHP.git"
        }
    ],
    "require": {
        "php-64bit": "8.1 || 8.2 || 8.3",
        "composer": "*",
        "adbario/php-dot-notation": "3.3.0",
        "akrabat/ip-address-middleware": "2.6.1",
        "apereo/phpcas": "1.6.1",
        "bigfish/pdf417": "1.0.0",
        "brick/math": "0.12.3",
        "cocur/slugify": "4.6.0",
        "consoletvs/profanity": "3.5.1",
        "dompdf/dompdf": "2.0.8",
        "egulias/email-validator": "4.0.4",
        "eluceo/ical": "2.14.0",
        "firebase/php-jwt": "6.11.1",
        "guzzlehttp/guzzle": "7.9.3",
        "hidehalo/nanoid-php": "2.0.0",
        "illuminate/database": "v10.48.28",
        "illuminate/events": "v10.48.28",
        "illuminate/pagination": "v10.48.28",
        "illuminate/support": "v10.48.28",
        "league/csv": "9.8.0",
        "league/flysystem": "3.29.1",
        "league/flysystem-aws-s3-v3": "3.29.0",
        "league/flysystem-ftp": "3.29.0",
        "league/flysystem-sftp-v3": "3.29.0",
        "league/mime-type-detection": "1.16.0",
        "mailjet/mailjet-apiv3-php": "1.6.5",
        "monolog/monolog": "3.9.0",
        "nesbot/carbon": "2.73.0",
        "odan/session": "~6.1.0",
        "onelogin/php-saml": "4.3.0",
        "php-di/php-di": "7.0.11",
        "phpmailer/phpmailer": "v6.10.0",
        "psr/container": "2.0.2",
        "ramsey/uuid": "4.8.1",
        "respect/validation": "2.8.0",
        "robmorgan/phinx": "0.16.9",
        "slim/http": "1.3.0",
        "slim/http-cache": "1.2.0",
        "slim/psr7": "1.7.1",
        "slim/slim": "4.14.0",
        "slim/twig-view": "3.4.1",
        "soundasleep/html2text": "2.1.0",
        "symfony/cache": "v6.4.21",
        "symfony/console": "v6.4.22",
        "symfony/filesystem": "6.4.24",
        "symfony/finder": "v6.4.17",
        "symfony/yaml": "v6.4.21",
        "twig/html-extra": "v3.21.0",
        "twig/intl-extra": "v3.21.0",
        "twig/string-extra": "v3.21.0",
        "twig/twig": "v3.21.1",
        "vlucas/phpdotenv": "5.6.2",
        "webmozart/assert": "1.11.0",
        "zircote/swagger-php": "5.1.3"
    },
    "require-dev": {
        "ifsnop/mysqldump-php": "2.12.0",
        "php-mock/php-mock": "~2.6.0",
        "phpcompatibility/php-compatibility": "9.3.5",
        "phpstan/extension-installer": "1.4.3",
        "phpstan/phpstan": "2.1.17",
        "phpstan/phpstan-deprecation-rules": "2.0.3",
        "phpstan/phpstan-phpunit": "2.0.6",
        "phpstan/phpstan-webmozart-assert": "2.0.0",
        "phpunit/phpunit": "10.5.46",
        "slevomat/coding-standard": "8.19.1",
        "spatie/phpunit-snapshot-assertions": "5.2.0",
        "squizlabs/php_codesniffer": "3.13.2",
        "symfony/var-dumper": "v6.4.21"
    },
    "autoload": {
        "psr-4": {
            "Loxya\\": "src/App",
            "Loxya\\Tests\\": "tests",
            "Loxya\\Tests\\Fixtures\\": "tests/fixtures"
        }
    },
    "config": {
        "vendor-dir": "vendors",
        "process-timeout": 0,
        "sort-packages": true,
        "platform": {
            "php": "8.1"
        },
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "phpstan/extension-installer": true
        }
    }
}
